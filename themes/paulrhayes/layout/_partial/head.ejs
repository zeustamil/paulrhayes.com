<% if (page.experiment) { %>
  <!-- Hey there, thanks for looking at the source. Skip down until you see the experiment start comments -->
<% } %>

<meta charset="UTF-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<%
var title = page.title;

if (is_archive()){
  title = 'Archives';

  if (is_month()){
    title += ': ' + page.year + '/' + page.month;
  } else if (is_year()){
    title += ': ' + page.year;
  }
} else if (is_category()){
  title = 'Category: ' + page.category;
} else if (is_tag()){
  title = 'Tag: ' + page.tag;
} else if (page.experiment) {
  title = 'Experiment: ' + title
} else if (page.layout == 'photo') {
  title = 'Photo: ' + title
}
%>
<title><% if (title){ %><%= title %> | <% } %><%= config.title %></title>
<meta name="description" content="I design and build websites from Brighton &amp; London. I take photos too." />
<meta name="google-site-verification" content="KXou1BjZqpDc9x8CuCA6epEKcxttO_EGrdhHSkqgEOI" />
